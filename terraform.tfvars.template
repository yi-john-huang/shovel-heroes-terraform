# Terraform Variables Template
# Copy this file to terraform.tfvars and fill in your actual values
# Never commit terraform.tfvars files to version control

# Basic Configuration
environment  = "production"
project_name = "shovel-heros"
aws_region   = "ap-east-2"

# Environment Configuration
env_vars = {
  env_name = "production"
  env_type = "prod"
}

# Secrets (use actual values - these will be excluded from version control)
secrets = {
  database_password = "CHANGE_THIS_SECURE_PASSWORD"
}