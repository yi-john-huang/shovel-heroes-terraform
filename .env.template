# Terraform Environment Variables Template
# Copy this file to .env and fill in your actual values
# Never commit .env files to version control

# AWS Configuration
AWS_PROFILE=your-aws-profile
AWS_REGION=ap-east-2
AWS_DEFAULT_REGION=ap-east-2

# Terraform Variables
TF_VAR_environment=production
TF_VAR_project_name=shovel-heros
TF_VAR_aws_region=ap-east-2
TF_VAR_primary_region=ap-east-2

# Database Secrets
TF_VAR_secrets_database_password=CHANGE_THIS_SECURE_PASSWORD